FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0.1 0 0);

boundaryField
{
    floor
    {
        type            noSlip;
    }
    ceiling
    {
        type            noSlip;
    }
    inlet
    {
        type            fixedValue;
        value           uniform (0.1 0 0);
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform (0 0 0);
        value           uniform (0.1 0 0);
    }
    fixedWalls
    {
        type            empty;
    }
    baffle1D0
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    baffle1D1
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    baffle3D0
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    baffle3D1
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
}


